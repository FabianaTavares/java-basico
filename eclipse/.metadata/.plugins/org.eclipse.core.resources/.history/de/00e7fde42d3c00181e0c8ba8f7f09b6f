		this.numConta = numConta;
	}
	public double getSaldo() {
		return Saldo;
	}
	public void setSaldo(double saldo) {
		Saldo = saldo;
	}
	
	public void sacar() {
		
	}
	
	public void depositar() {
		
	}
	
	
}
